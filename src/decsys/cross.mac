	CAIL	CHR,"0"
	CAILE	CHR,"9"
	JRST	FLTG5A
	IMULI	SYM,^D10
	ADDI	SYM,-"0"(CHR)
	AOJA	T1,FLTG5

FLTG5A:	TLZE	T1,(1B0)
	MOVNS	SYM
	SKIPN	T1
	TLO	FLG,FLTFLG
	ADD	T2,SYM
