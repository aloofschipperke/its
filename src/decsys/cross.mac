	RETURN

M68A0A:	SPUSH	[0]		;INIT MODE INFO
	CAIN	CHR,"#"		;IMMED?
	JRST	M68A20		;YES - HANDLE

	CALL	EXPR		;GET EXPRESSION
	 ERRSET	ERR.Q
	SPOP	T4		;GET MODES
	CAIN	CHR,","		;CHECK FOR INDEXED
	JRST	M68A10
	HRRI	T4,.M68A2	;NO - JUST SAY ADDRS
	JRST	M65AR1		;COMMON EXIT

