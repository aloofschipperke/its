	JUMPE	SYM,FLTG12
	TDZA	T3,T3
FLTG7:	ASHC	SYM,1
	TLNN	SYM,200000
	SOJA	T3,FLTG7
	JUMPL	T2,FLTG9
FLTG8:	SOJL	T2,FLTG10
	MOVEM	SYM,FLTTMP
	MOVEM	T1,FLTTMP+1
	ASHC	SYM,-2
	ADD	SYM,FLTTMP
	ADD	T1,FLTTMP+1
	TLZE	T1,(1B0)
	ADDI	SYM,1
	TLNE	SYM,(1B0)
	CALL	FLTG20
	ADDI	T3,3
	JRST	FLTG8
