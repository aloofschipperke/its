	SOS	MCATMP
	CAIE	T3,DCMAC
	JRST	MCALL4
	AOS	T3,MCATMP
	CAIE	T3,1
	JRST	MCALL4
	CALL	GSARG
	 JRST	MCALL3
	CALL	MSRCH
	 CAIA
	TRNE	T1,-1
	JRST	MCALL4
	CALL	MACROF
	TLNE	FLG,ENDFLG
	JRST	MCALL6
	SOSLE	MCACNT
	JRST	MCALL3
	JRST	MCALL7

MCALL6:	ERRSET	ERR.A
MCALL7:	SETZM	MCACNT
