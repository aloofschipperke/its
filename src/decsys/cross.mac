TIMTST:	JFCL
	 CAIA
	AOS	0(P)
	SPOP	TIMTMP
	SPUSH	T2
	SETZM	T2
	RUNTIM	T2,
	EXCH	T2,0(P)
	CALL	@TIMTMP
	 CAIA
	AOS	-1(P)
	EXCH	T2,-1(P)
	MOVEM	T2,TIMTMP
	SETZM	T2
	RUNTIM	T2,
	SUB	T2,0(P)
	ADDM	T2,TIMTIM
	AOS	TIMCNT
	SPOP	T2
	SPOP	T2
