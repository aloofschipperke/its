	CALL	MCFDIV		;  YES
	HLRZ	T3,0(P)	;NO,RETRIEVE NUMBER
	HLRZ	T4,T1		;GET CONSTANT
	ADD	T3,T4		;ADD IT IN
	WCIMT	0(T3)		;STORE IT
