	CALL	FLTG20
	SPUSH	T1+1
	DIV	SYM,[^D10B4]
	DIV	T1,[^D10B4]
	SPOP	T1+1
	SUBI	T3,4
	AOJL	T2,FLTG9
FLTG10:	SPUSH	T3		;STACK EXPONENT
